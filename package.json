{
  "name": "avid",
  "version": "1.2.2",
  "private": true,
  "engines": {
    "pnpm": "^9"
  },
  "scripts": {
    "prepare": "husky",
    "start": "EXPO_NO_DOTENV=1 expo start",
    "prebuild": "EXPO_NO_DOTENV=1 expo prebuild --clean",
    "prebuild:ios": "EXPO_NO_DOTENV=1 expo prebuild --clean -p ios",
    "prebuild:ios:preview": "APP_ENV=preview pnpm prebuild:ios",
    "prebuild:ios:prod": "APP_ENV=production pnpm prebuild:ios",
    "prebuild:android": "EXPO_NO_DOTENV=1 expo prebuild --clean -p android",
    "prebuild:android:preview": "APP_ENV=preview pnpm prebuild:android",
    "prebuild:android:prod": "APP_ENV=production pnpm prebuild:android",
    "ios": "EXPO_NO_DOTENV=1 expo run:ios",
    "ios:preview": "APP_ENV=preview pnpm ios",
    "ios:prod": "APP_ENV=production pnpm ios",
    "ios:release": "EXPO_NO_DOTENV=1 expo run:ios --configuration Release",
    "ios:preview:release": "APP_ENV=preview pnpm ios:release",
    "ios:prod:release": "APP_ENV=production pnpm ios:release",
    "android": "EXPO_NO_DOTENV=1 expo run:android",
    "android:preview": "APP_ENV=preview pnpm android",
    "android:prod": "APP_ENV=production pnpm android",
    "android:release": "EXPO_NO_DOTENV=1 expo run:android --variant release",
    "android:preview:release": "APP_ENV=preview pnpm android:release",
    "android:prod:release": "APP_ENV=production pnpm android:release",
    "web": "expo start --web",
    "doctor": "npx expo-doctor@latest",
    "check": "pnpm fmt && pnpm ts && pnpm lint",
    "fmt": "prettier . --check",
    "ts": "tsc --project .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@tabler/icons-react-native": "^3.19.0",
    "@tanstack/react-query": "^5.56.2",
    "class-variance-authority": "^0.7.0",
    "expo": "^51.0.34",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-font": "~12.0.10",
    "expo-linking": "~6.3.1",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.12",
    "twrnc": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@dev-plugins/react-navigation": "^0.0.6",
    "@dev-plugins/react-query": "^0.0.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@react-native/eslint-config": "^0.73.2",
    "@swc-node/register": "^1.10.9",
    "@tanstack/eslint-plugin-query": "^5.58.1",
    "@tsconfig/react-native": "^3.0.5",
    "@tsconfig/strictest": "^2.0.5",
    "@types/react": "~18.2.79",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3",
    "zod": "^3.23.8"
  }
}
