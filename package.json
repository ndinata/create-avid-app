{
  "name": "navy",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "prepare": "husky",
    "start": "EXPO_NO_DOTENV=1 expo start",
    "prebuild": "EXPO_NO_DOTENV=1 expo prebuild --clean",
    "prebuild:ios": "EXPO_NO_DOTENV=1 expo prebuild --clean -p ios",
    "prebuild:ios:preview": "APP_ENV=preview npm run prebuild:ios",
    "prebuild:ios:prod": "APP_ENV=production npm run prebuild:ios",
    "prebuild:android": "EXPO_NO_DOTENV=1 expo prebuild --clean -p android",
    "prebuild:android:preview": "APP_ENV=preview npm run prebuild:android",
    "prebuild:android:prod": "APP_ENV=production npm run prebuild:android",
    "ios": "EXPO_NO_DOTENV=1 expo run:ios",
    "ios:preview": "APP_ENV=preview npm run ios",
    "ios:prod": "APP_ENV=production npm run ios",
    "ios:release": "EXPO_NO_DOTENV=1 expo run:ios --configuration Release",
    "ios:preview:release": "APP_ENV=preview npm run ios:release",
    "ios:prod:release": "APP_ENV=production npm run ios:release",
    "android": "EXPO_NO_DOTENV=1 expo run:android",
    "android:preview": "APP_ENV=preview npm run android",
    "android:prod": "APP_ENV=production npm run android",
    "android:release": "EXPO_NO_DOTENV=1 expo run:android --variant release",
    "android:preview:release": "APP_ENV=preview npm run android:release",
    "android:prod:release": "APP_ENV=production npm run android:release",

    "web": "expo start --web",
    "check": "npm run fmt && npm run tsc && npm run lint",
    "fmt": "prettier . --check",
    "tsc": "tsc --project .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.10",
    "@tabler/icons-react-native": "^3.1.0",
    "@tanstack/react-query": "^5.29.2",
    "expo": "~50.0.15",
    "expo-application": "~5.8.3",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6",
    "twrnc": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@dev-plugins/react-navigation": "^0.0.6",
    "@dev-plugins/react-query": "^0.0.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@react-native/eslint-config": "^0.73.2",
    "@swc-node/register": "^1.9.0",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@tsconfig/react-native": "^3.0.3",
    "@tsconfig/strictest": "^2.0.3",
    "@types/react": "~18.2.45",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3",
    "zod": "^3.22.4"
  },
  "private": true
}
