{
  "extends": [
    "expo/tsconfig.base",
    "@tsconfig/react-native/tsconfig",
    "@tsconfig/strictest/tsconfig"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "verbatimModuleSyntax": true,
    "exactOptionalPropertyTypes": false,
    "useUnknownInCatchVariables": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["ios/", "android/"]
}
