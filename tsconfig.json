{
  "extends": [
    "expo/tsconfig.base",
    "@tsconfig/react-native/tsconfig",
    "@tsconfig/strictest/tsconfig"
  ],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "useUnknownInCatchVariables": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["ios/", "android/"]
}
